<%= link_to "See all users", users_path %><br><br>
<%= link_to "New Post", new_post_path %><br><br>
<% @posts.each do |post|%>
  <%= "Post ##{post.id}"%><br>
  <%= "Author: " %>
  <%= post.user.email %><br>
  <%= post.content %><br>
  <%= button_to "Like", likes_path({"like"=>{post_id: post}}) %>
<% end %>
