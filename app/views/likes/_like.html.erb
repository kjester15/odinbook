<div class="like-container">
  <% like = current_user.likes.find_by(post_id: post.id) %>
  <% if like.nil? %>
    <%= button_to "", likes_path({"like"=>{post_id: post}}), class: "like-thumb-outline" %>
  <% else %>
    <%= button_to "", like_path(like), method: :delete, class: "like-thumb" %>
  <% end %>
</div>